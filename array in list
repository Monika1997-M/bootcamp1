package BootCamp;
import java.util.Scanner;
public class Arraynumber {
public static void main(String args[])//main
{
//initializing an array
	int[] array={10,20,32,50,60};
	int i;//instance variabes
	int x=-1;
	Boolean element=false;
	Scanner sc=new Scanner(System.in);
	int n=sc.nextInt();
	for(i=0;i<=array.length-1;i++)//loop iteration
	{
		if(n==array[i])
		{
			element=true;
			x=i;
			break;
		}
	}
	if(element)
//checking the numbers present in the array or not
	{
		System.out.println("found"+ "@pos"+x);
	}
	else
	{
		System.out.println("not found");
	}
}
}


